@using Group.Salto.Common.Constants
@using Group.Salto.SOM.Web.Extensions

@{
    var nService = ViewData;
}

@if (nService.Model == 0)
{
        <div class="col p-0">
            <div id="nodataview" class="alert alert-info text-center">
                <i class="fa fa-info-circle fa-5x mt-3"></i>
                <p class="lead">@Html.GetLocalizedText(LocalizationsConstants.NoData)</p>
            </div>
        </div>
}
else
{
    <div id="nodataview" class="alert alert-info text-center">
        <i class="fa fa-info-circle fa-5x mt-3"></i>
        <p class="lead">@Html.GetLocalizedText(LocalizationsConstants.NoData)</p>
    </div>
}